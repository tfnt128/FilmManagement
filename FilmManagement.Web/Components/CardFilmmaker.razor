<MudCard>
    <MudCardMedia Image="images/cardFilmmaker.png"></MudCardMedia>
    <MudCardHeader>
        <CardHeaderContent>
            <MudText Typo="Typo.h6">@Filmmaker!.Name</MudText>
        </CardHeaderContent>
    </MudCardHeader>
    <MudCardContent>
        <MudText Typo="Typo.body2">@Filmmaker!.Bio</MudText>
    </MudCardContent>
</MudCard>
@code {
    [Parameter]
    public FilmmakerResponse? Filmmaker { get; set; }
}
